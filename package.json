{
  "name": "webpack-react-config",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --env mode=production",
    "eslint": "eslint --ext .jsx --ext .js src/",
    "eslint:fix": "eslint --ext .jsx --ext .js src/ --fix",
    "stylelint": "stylelint '**/*{.css,.scss,.less}'",
    "stylelint:fix": "stylelint '**/*{.css,.scss,.less}' --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.21.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "react/prefer-stateless-function": "off",
      "react/function-component-definition": "off"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
